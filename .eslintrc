{
    "extends": [
      "airbnb",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "settings": {
      "import/resolver": {
        "webpack": { "config": "./webpack/webpack.prod.config.js" }
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "global-require": "off",
      "import/prefer-default-export": "off",
      "import/no-named-as-default": "off",
      "import/no-extraneous-dependencies": "off"
    }
}
