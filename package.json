{
  "name": "react-boilerplate-lite",
  "version": "0.0.0",
  "description": "Minimal boilerplate for React apps",
  "scripts": {
    "start": "node server/server.js",
    "start:dashboard": "webpack-dashboard node server/server.js",
    "postinstall": "npm run build",
    "build": "webpack --config webpack/webpack.prod.config.js",
    "clean": "rm -rf build",
    "lint": "eslint src --ext .js*",
    "test": "jest"
  },
  "author": "Michał Załęcki <michal@michalzalecki.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "compression": "^1.7.3",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "morgan": "^1.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.3.12",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3"
  }
}
