{
  "name": "react-boilerplate-lite",
  "version": "0.0.0",
  "description": "Minimal boilerplate for React apps",
  "engines": {
    "node": "^10.15.0",
    "npm": "^6.6.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "start:dashboard": "webpack-dashboard node server/server.js",
    "postinstall": "npm run build",
    "build": "webpack --config webpack/webpack.prod.config.js",
    "clean": "rm -rf build",
    "lint": "eslint src --ext .js*",
    "test": "jest"
  },
  "author": "Michał Załęcki <michal@michalzalecki.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "compression": "^1.7.4",
    "css-loader": "^2.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "webpack-dashboard": "^3.0.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3"
  }
}
